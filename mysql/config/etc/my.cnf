[client]
# CLIENT #
port                            = 3306

[mysqld]
# GENERAL #
default_storage_engine          = InnoDB
wait_timeout                    = 3600
explicit_defaults_for_timestamp = 1
sql_mode                        = STRICT_TRANS_TABLES,ERROR_FOR_DIVISION_BY_ZERO,NO_AUTO_CREATE_USER,NO_AUTO_VALUE_ON_ZERO,NO_ENGINE_SUBSTITUTION,NO_ZERO_DATE,NO_ZERO_IN_DATE,ONLY_FULL_GROUP_BY,PIPES_AS_CONCAT
